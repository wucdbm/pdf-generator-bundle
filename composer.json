{
    "name": "wucdbm/pdf-generator-bundle",
    "type": "symfony-bundle",
    "description": "A WKHTMLTOPDF based PDF Generator",
    "keywords": ["symfony pdf generator", "html to pdf", "symfony html pdf", "pdf bundle", "wucdbm"],
    "authors": [
        {
            "name": "Martin Kirilov",
            "email": "wucdbm@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.0",
        "symfony/symfony": "~2.0|~3.0"
    },
    "require-dev": {
        "h4cc/wkhtmltopdf-i386": "0.12.3"
    },
    "suggest": {
        "h4cc/wkhtmltopdf-i386": "For using wkhtmltopdf without installing it globally. This package provides a 32 bit binary."
    },
    "autoload": {
        "psr-4": { "Wucdbm\\Bundle\\PdfGeneratorBundle\\": "" }
    },
    "license": "GPL-3.0"
}